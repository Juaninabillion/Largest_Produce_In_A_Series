<html>
<body>
<script>
largest_product();
function largest_product(){
var num = ''

var highbound = 0;
var start = 0; 
var end = 0;
var max = 0;

num.toString().split('')

for (var i = 0;i<num.length-13;i++)
	{
		start = i;
    end = (i+13)
    var p = 1;
    for(start;start<end;start++)
    {
      p = p * num[start]
    }
    if(max< p){max = p}
	
  }
 alert(max); 
}
</script>
</body>

</html>
